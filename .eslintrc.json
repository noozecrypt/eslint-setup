{
	"env": {
		"browser": true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"find-payments": "error",
		"no-restricted-imports": ["error", "underscore"],
		"eqeqeq": "error",
		"no-console": "error",
		"consistent-return": "error",
		"indent": ["warn", "tab"],
		"linebreak-style": ["error", "unix"],
		"quotes": ["warn", "single"],
		"semi": ["error", "always"]
	}
}
